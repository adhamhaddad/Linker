{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["ES6", "DOM"],
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["node_module", "./dist"]
}
